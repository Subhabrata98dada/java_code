package com.nt.models;

import jakarta.persistence.Entity;
import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;


@Entity(name = "employee")
@Setter
@Getter
@AllArgsConstructor
@NoArgsConstructor
public class Employee {
    Integer id;
    String name;
    Long mobile;
    String email;
    String job;
    Integer deptno;
    Double salary;
}
